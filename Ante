local Folder
local Torso
local Warned = false

game.Players.PlayerAdded:Connect(function(Player)
	for i, v in pairs(watchout)do
		if Player.Name == v then
			Warned = true
			LOG("HIGHER UP JOINED: "..Player.Name)
		end
	end
	for i,v in pairs(nosyasses)do
		if Player.Name == v then
			Warned = true
			LOG("NOSEY ASS JOINED: "..Player.Name)
		end
	end
end)
for i,v in pairs(game.Players:GetChildren())do
	for O, P in pairs(watchout)do
		if P == v.Name then
			Warned = true
			LOG("HIGHER UP JOINED: "..v.Name)
		end
	end
end

local function Bind()
	local suc, reas = pcall(function()
		if game.Workspace:FindFirstChild("ServiceS") then
			print("Passed")
			Folder = game.Workspace.ServiceS
		else
			Folder = Instance.new("Folder", workspace)
			Folder.Name = "ServiceS"
			game.Players.LocalPlayer.CharacterAdded:Connect(function(Plr)
				Folder:ClearAllChildren()
				Torso = nil
				local T = Plr:WaitForChild("Torso"):Clone()
				for i,v in pairs(T:GetChildren())do
					if v.Name ~= "Neck" then
						v:Destroy()
					elseif v.Name == "Neck" then
						v.Name = "Bind"
					end
					T.Name = "Hue"
					T.CanCollide = false
					T.Parent = Folder
					T.Transparency = 1
				end
			end)
		end
		if Folder:FindFirstChild("Hue") then
			print("Passed 2")
			Torso = Folder.Hue
		else
			print("lol")
			Torso = game.Players.LocalPlayer.Character:FindFirstChild("Torso"):Clone()
			print(Torso.Parent)
			for i,v in pairs(Torso:GetChildren())do
				if v.Name ~= "Neck" then
					v:Destroy()
				elseif v.Name == "Neck" then
					v.Name = "Bind"
				end
			end
			Torso.Transparency = 1
			Torso.CanCollide = false
			Torso.Name = "Hue"
			Torso.Parent = Folder

		end
	end)
	if suc == false then 
		print(reas)
		game.Players.LocalPlayer.Character:FindFirstChild("Humanoid").Health = 0
	else
		print("passed")
	end
end

Bind()

local function TP(Position)
	local Tele = Instance.new("BodyPosition")
	game.Workspace.ServiceS.Hue.Anchored = false
	Tele.P = 1000000
	Tele.D = 1250
	Tele.MaxForce = Vector3.new(math.huge,math.huge,math.huge)
	Tele.Position = Position
	Tele.Parent = game.Workspace.ServiceS.Hue
	task.wait(0.1)
	game.Workspace.ServiceS.Hue.Anchored = true
	Tele:Destroy()
end

local LP = game.Players.LocalPlayer
local HRP = LP.Character:WaitForChild("HumanoidRootPart")
local lag = HRP:Clone()
local Head = LP.Character:WaitForChild("Head")
local tor = LP.Character:WaitForChild("Torso")
local pos = tor.Position
lag.Parent = LP.Character
LP.Character:WaitForChild("Animate"):Destroy()
for i,v in pairs(Head:GetChildren())do
	if v.Name == "Health" then
		v:Destroy()
	elseif v.Name == "Streak" then
		v:Destroy()
	end
end



wait(0.5)
for i,v in pairs(game.Workspace:GetChildren())do
	if v.Name == "Healing" then
		Torso.Anchored = false
		wait(0.1)
		TP(game.Workspace.Healing.Position)
	elseif v.Name == "Healing2" then
		TP(game.Workspace.Healing2.Position)
	elseif v.Name == "Healing3" then
		TP(game.Workspace.Healing3.Position)
		wait(0.2)
		TP(pos)
		Torso.Anchored = true
	end
	wait(0.15)
end


game.Workspace.ChildAdded:connect(function(v)
	if v.Name == "Healing" then
		print("HA")
		TP(game.Workspace.Healing.Position)
	elseif v.Name == "Healing2" then
		wait(0.2)
		TP(game.Workspace.Healing2.Position)
	elseif v.Name == "Healing3" then
		wait(0.4)
		TP(game.Workspace.Healing3.Position)
		wait(0.2)
		TP(pos)
		wait(0.1)
		for i,v in pairs(game.Workspace:GetChildren())do
			if v.Name == "Healing" then
				TP(v.Position)
				wait(0.2)
				TP(pos)
			elseif v.Name == "Healing2" then
				TP(v.Position)
				wait(0.2)
				TP(pos)
			elseif v.Name == "Healing3" then
				TP(v.Position)
				wait(0.2)
				TP(pos)
			end
			wait(0.2)
		end
	end


end)
game.Players.LocalPlayer.Character.Humanoid.Died:Connect(function()
	Warned = true
	game.Players.LocalPlayer:Kick("you died")
	Number.Accounts = Number.Accounts - 1
	writefile("NumberAcc.Ant", httpService:JSONEncode(Number))
	LOG("Player had died")
	messagebox("Check Discord Server for details","Something occurred,",790)
	game:Shutdown()

end)
game.NetworkClient.ChildRemoved:Connect(function()
	if Warned ~= true then
		LOG("either player disconnected or was kicked")
	end
end)
local afk = game:GetService("VirtualUser")
game:GetService("Players").LocalPlayer.Idled:connect(function()
	afk:Button2Down(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
	wait(1)
	afk:Button2Up(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
end)

local words = wordlist:split(" ")

local wordsaid = {}
local mag
for d,c in pairs(game.Players:GetChildren()) do
	c.Chatted:Connect(function(lol)
		mag = (c.Character.Torso.Position - game.Players.LocalPlayer.Character.Torso.Position).Magnitude
		local chatted = string.lower(lol)
		local check = chatted:split(" ")
		local flag = false
		for i,v in pairs(check) do
			for a,b in pairs(words)do
				if string.find(v,b) then
					table.insert(wordsaid,v)
					flag = true
				end
			end
		end
		if flag == true then
			if mag < 25 then
				print(wordsaid)
				local FlagWords = table.concat(wordsaid,'\n')
				local Data = {
					['username'] = "WatchBot",
					["content"] = "@everyone, Victim: "..game.Players.LocalPlayer.Name,
					["embeds"] = {
						{

							['title'] = "User: "..game.Players.LocalPlayer.Name,
							['color'] = 12983122,
							['fields'] = {
								{
									['name'] = "Reason of kick",
									['value'] = "Flagged word was said",
									['inline'] = true
								},
								{
									['name'] = "Matched words that were flagged:",
									['value'] = FlagWords,
									['inline'] = false
								},
								{
									['name'] = "Sender",
									['value'] = c.Name,
									['inline'] = true
								},
								{
									['name'] = "Distance from player",
									['value'] = tostring(mag).."m",
									['inline'] = true
								}

							},
						}
					}
				}

				local response = syn.request(
					{
						Url = "https://discordapp.com/api/webhooks/926356490028220417/fJoDufLd_ECj3IE5J5dJGDoF9T92sM4U_b14hBFZM4LVlf3YESvR-vXcoBzECRa29oqu",  -- This website helps debug HTTP requests
						Method = "POST",
						Headers = {
							["Content-Type"] = "application/json"  -- When sending JSON, set this!
						},
						Body = game:GetService'HttpService':JSONEncode(Data)
					}
				)
				wordsaid = {}
				Warned = true
				game.Players.LocalPlayer:Kick()
				messagebox("Something occurred, ","Check Discord Server for details",790)
				game:Shutdown()
			end
		end


	end)
end
