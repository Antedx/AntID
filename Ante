local httpService = game:GetService("HttpService")
if not isfile("NumberAcc.Ant") then
	local Save = {
		["Accounts"] = 0
	}
	writefile("NumberAcc.Ant", httpService:JSONEncode(Save))

end
if not isfile("Counter.Ant") then
	local Save = {
		["Count"] = 0,
		["usercount"] = 0,
		["Users"] = {"ROBLOX"}
	}
	writefile("Counter.Ant", httpService:JSONEncode(Save))
end
local Number = httpService:JSONDecode(readfile("NumberAcc.Ant"))
local savestate = httpService:JSONDecode(readfile("Counter.Ant"))
Number.Accounts = Number.Accounts + 1
writefile("NumberAcc.Ant", httpService:JSONEncode(Number))
Number = httpService:JSONDecode(readfile("NumberAcc.Ant"))
local Time = game.Players.LocalPlayer.leaderstats.Time


coroutine.wrap(function()
	while task.wait()do
		savestate = httpService:JSONDecode(readfile("Counter.Ant"))
		Number = httpService:JSONDecode(readfile("NumberAcc.Ant"))
	end
end)()
coroutine.wrap(function()
	while task.wait(TimeBankRefresh) do
		local suc,rea = pcall(function()

			if not table.find(savestate.Users, game.Players.LocalPlayer.Name) then
				table.insert(savestate.Users, game.Players.LocalPlayer.Name)
				savestate.usercount = savestate.usercount + 1
				savestate.Count = savestate.Count + Time.Value
				writefile("Counter.Ant", httpService:JSONEncode(savestate))
				savestate = httpService:JSONDecode(readfile("Counter.Ant"))
			else
				print("already in")

			end
			task.wait(0.2)
			if savestate.usercount >= Number.Accounts then
				local Data = {
					['username'] = "RiceBot",
					["content"] = name.."'s Total Time Amount",
					["embeds"] = {
						{

							['title'] = savestate.Count,
							['color'] = 12989122
						}
					}
				}

				local response = syn.request(
					{
						Url = "https://discordapp.com/api/webhooks/926304157361664061/kp31l-_f_kKFyy5cdtPqhMND35gyPZCIIzQlaTTAUzc0u2Ar-FHZLyUuZghrFHCOaI8d",  -- This website helps debug HTTP requests
						Method = "POST",
						Headers = {
							["Content-Type"] = "application/json"  -- When sending JSON, set this!
						},
						Body = game:GetService'HttpService':JSONEncode(Data)
					}
				)
				print(savestate.Count)
				local Save = {
					["Count"] = 0,
					["usercount"] = 0,
					["Users"] = {"ROBLOX"}
				}
				writefile("Counter.Ant", httpService:JSONEncode(Save))
				savestate = httpService:JSONDecode(readfile("Counter.Ant"))
			end

		end)
		if suc == false then 
			rconsolewarn(rea.."\n")
			
		else

		end
	end
end)()







